{
  "name": "synq-extension",
  "displayName": "SynQ",
  "version": "0.0.1",
  "description": "The browser extension for the SynQ app.",
  "author": "SynQ, LLC",
  "scripts": {
    "dev": "plasmo dev",
    "build": "npx rimraf ./build ./.plasmo && plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@plasmohq/messaging": "^0.5.0",
    "@synq/music-service-clients": "1.0.5",
    "@synq/ui": "0.0.4-alpha.5",
    "plasmo": "0.75.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-fast-marquee": "^1.6.0",
    "react-router-dom": "^6.14.0",
    "redux": "^4.2.1",
    "styled-components": "^6.0.3",
    "usehooks-ts": "^2.9.1",
    "uuid": "^9.0.0",
    "waait": "^1.0.5"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.4",
    "@types/chrome": "0.0.233",
    "@types/node": "18.15.12",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.2",
    "prettier": "2.8.7",
    "rimraf": "^5.0.1",
    "typescript": "5.0.4"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*",
      "*://*/*"
    ],
    "permissions": [
      "tabs",
      "activeTab"
    ]
  }
}
